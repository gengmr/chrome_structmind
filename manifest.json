{
  "name": "StructMind",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": [
    "sidePanel"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "点击打开StructMind",
    "default_icon": {
      "16": "icons/icon.png",
      "48": "icons/icon.png",
      "128": "icons/icon.png"
    }
  },
  "side_panel": {
    "default_path": "panel.html"
  }
}
